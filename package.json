{
  "name": "@jd/jm-design",
  "version": "0.6.1",
  "description": "JM Mobile UI components for vue.js",
  "contributors": [
    "zhongzhijie <zhongzhijie@jd.com>"
  ],
  "scripts": {
    "dev": "npm run build:icon & cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.docs.conf.js",
    "build:docs": "rimraf docs && webpack --progress --config build/webpack.docs.conf.js",
    "build:lib": "webpack --progress --config build/webpack.lib.conf.js",
    "build:components": "webpack --progress --config build/webpack.components.conf.js",
    "build:theme": "node build/mixCssFile.js & gulp build --gulpfile build/build-theme.js && node build/build-style-entry.js",
    "build:locale": "cross-env BABEL_ENV=utils babel src/locale/lang --out-dir lib/locale/lang && node build/build-locale.js",
    "build:icon": "node build/iconInit.js",
    "build:dist": "rimraf lib && npm run build:lib && npm run build:components && npm run build:theme && npm run build:locale",
    "unit": "cross-env BABEL_ENV=test NODE_ENV=development karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs"
  },
  "main": "./lib/index.js",
  "style": [],
  "keywords": [
    "vue",
    "jm",
    "component"
  ],
  "files": [
    "lib",
    "packages",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "http://git.jd.com/JM-FE/jm-design.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint \"{src,packages,examples,examples-m}/**/*.{js,vue}\""
    }
  },
  "peerDependencies": {
    "vue": "^2.5.2"
  },
