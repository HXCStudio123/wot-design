{
  "name": "@jd/jm-design",
  "version": "0.1.1",
  "description": "JM Mobile UI components for vue.js",
  "contributors": [
    "zhongzhijie <zhongzhijie@jd.com>"
  ],
  "scripts": {
    "dev": "npm run build:icon & cross-env NODE_ENV=develpoment webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "build:docs": "rimraf docs && webpack --progress build/webpack.docs.conf.js",
    "build:lib": "webpack --progress build/webpack.lib.conf.js",
    "build:components": "webpack --progress build/webpack.components.conf.js",
    "build:theme": "node build/mixCssFile.js",
    "build:icon": "node build/iconInit.js",
    "build": "rimraf lib && npm run build:theme && npm run build:lib && npm run build:comp",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs"
  },
  "main": "./lib/index.js",
  "style": [],
  "keywords": [
    "vue",
    "jm",
    "component"
  ],
  "files": [
    "lib",
    "packages",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "http://git.jd.com/JM-FE/jm-design.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint \"{src,packages,examples,examples-m}/**/*.{js,vue}\""
    }
  },
